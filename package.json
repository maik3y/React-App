{
  "name": "perfection-tv",
  "version": "1.0.0",
  "description": "Website for perfection.tv",
  "repository": "https://github.com/PerfectionCSGO/perfection-tv.git",
  "author": "perfection.tv <perfectioncsgo@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:clean": "rimraf ./dist",
    "build:renderer:hot": "webpack-dev-server --progress --hot --open --config ./scripts/webpack/webpack.config.development",
    "build:renderer": "webpack --progress --config ./scripts/webpack/webpack.config.production",
    "build": "yarn build:clean && yarn build:renderer",
    "ci": "yarn build:clean && yarn build:renderer && yarn eslint-check && yarn eslint && yarn stylelint && yarn typedoc && yarn test",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "eslint": "eslint \"./**/*.{ts,tsx}\"",
    "prettier": "prettier --list-different \"./**/*.{tsx,jsx,ts,js,less,scss,css}\"",
    "start": "yarn build:clean && yarn build:renderer:hot",
    "stylelint": "stylelint \"./**/*.{scss,less,css}\"",
    "test": "jest --config jestconfig.json",
    "typedoc": "typedoc --out ./dist/documentation --excludeExternals --mode modules --module commonjs --target ES6 ./src"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.2.3",
    "core-js": "^3.2.1",
    "lodash": "^4.17.15",
    "mobx": "^5.13.0",
    "mobx-react-lite": "^1.4.1",
    "react": "^16.9.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.12.13",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.11.0",
    "terser-webpack-plugin": "^2.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  }
}
