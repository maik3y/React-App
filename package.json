{
  "name": "perfection-tv",
  "version": "1.0.0",
  "description": "Website for perfection.tv",
  "main": "src/index.js",
  "repository": "https://github.com/PerfectionCSGO/perfection-tv.git",
  "author": "perfection.tv <perfectioncsgo@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack --progress --config webpack.config.production",
    "start": "webpack-dev-server --progress --hot --open --config webpack.config.development",
    "clean": "rimraf dist",
    "test": "jest",
    "prettier": "prettier \"**/*.{ts,tsx,md,json}\" --list-different",
    "eslint": "eslint \"**/*.{ts,tsx}\"",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "stylelint": "stylelint \"./src/**/*.scss\""
  },
  "dependencies": {
    "@hot-loader/react-dom": "npm:@hot-loader/react-dom",
    "react": "^16.8.6",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-config": "^1.1.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.8.1",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-jest": "^24.6.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.6.0",
    "jest-haste-map": "^24.6.0",
    "jest-resolve": "^24.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.2",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-prettier": "^1.0.6",
    "stylelint-scss": "^3.5.4",
    "terser-webpack-plugin": "^1.2.3",
    "typescript": "^3.4.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "bail": 1,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index/index.tsx"
    ],
    "coverageDirectory": "dist/coverage",
    "coverageReporters": [
      "cobertura",
      "html",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  }
}
